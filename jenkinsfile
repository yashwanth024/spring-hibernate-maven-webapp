 pipeline {
        agent any
        tools {
                maven 'LOCALMAVEN'
        }
  stages {
        stage(scm) {
          steps {
                git 'https://github.com/avinasharuri/spring-hibernate-maven-webapp.git'
          }
        }

        stage (compile) {
          steps {
                sh 'mvn compile'
                }
                        }

        stage (test) {
          steps {
                sh 'mvn test'
                }
                }
      }
   }
